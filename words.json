{
    "Сәлемдескенде":{
        "title": "Greeting",
        
        
        "data":[
                {
                "phrase": "Сәлеметсіз бе?",
                "translation": "안녕하세요",
                "transcription": "[ан : нёң : һа : сэ : йо]",
                "sound": ""
                },
                
                {
                "phrase": "Қайырлы таң",
                "translation": "좋은 아침입니다",
                "transcription": "[чо : ын : а : тчим : им : ни : да]",
                "sound": ""
                },
                
                {
                "phrase": "Қайырлы күн",
                "translation": "좋은 오후입니다",
                "transcription": "[чо : ын : о : ху : им : ни : да]",
                "sound": ""
                },
                {
                "phrase": "Қайырлы кеш",
                "translation": "좋은 저녁입니다",
                "transcription": "[чо : ын : чо : нёк : им : ни : да]",
                "sound": ""
                },
                
                {
                "phrase": "Қайырлы түн",
                "translation": "좋은 밤 되세요",
                "transcription": "[чо : ын : пам : тэ : сэ : йо]",
                "sound": ""
                },
                
                {
                "phrase": "Қалыңыз қалай?",
                "translation": "어떻게 지내세요?",
                "transcription": "[от : то : ке : чи : нэ : сэ : йо]",
                "sound": ""
                },
                {
                "phrase": "Жақсымын, рахмет!",
                "translation": "잘 지내요, 고마워요!",
                "transcription": "[чаль : чи : нэ : йо : ко : ма : уо : йо]",
                "sound": ""
                },
                
                {
                "phrase": "Атыңыз кім?",
                "translation": "이름이 뭐예요?",
                "transcription": "[и : рым : и : мұо : йе : йо]",
                "sound": ""
                },
                
                {
                "phrase": "Менің атым …",
                "translation": "제 이름은 ….입니다",
                "transcription": "[че : и : рым : ын : … : им : ни : да]",
                "sound": ""
                },
                {
                "phrase": "Көріскенше!",
                "translation": "나중에 봐요!",
                "transcription": "[на : джуң : э : па : йо]",
                "sound": ""
                },
                
                {
                "phrase": "Сау болыңыз!",
                "translation": "안녕히 계세요 / 가세요",
                "transcription": "[ан : нёң : һи : кие : сэ : йо]",
                "sound": ""
                },
                
                {
                "phrase": "Өз-өзіңді күт",
                "translation": "조심하세요",
                "transcription": "[джо : щим : һа : сэ : йо]",
                "sound": ""
                },
                {
                "phrase": "Танысқанымға қуаныштымын",
                "translation": "만나서 반갑습니다",
                "transcription": "[ман : на : со : пан : гап : сым : ни : да]",
                "sound": ""
                },
                
                {
                "phrase": "Көріспегелі көп болыпты",
                "translation": "오랜만이에요",
                "transcription": "[о : рэн : ман : и : э : йо]",
                "sound": ""
                },
                
                {
                "phrase": "Кәрісше кішкене сөйлей аламын",
                "translation": " 한국어 조금 할 수 있어요",
                "transcription": "[хан : гуг : о : чо : гым : халь : су : ис : со : йо]",
                "sound": ""
                },
                {
                "phrase": "Кәрісше білмеймін",
                "translation": "한국어 못해요",
                "transcription": "[хан : гуг : о : мот : хэ : йо]",
                "sound": ""
                },{
                "phrase": "Мен Қазақстаннан келдім",
                "translation": "저는 카자흐스탄에서 왔어요",
                "transcription": "[чо : нын : ка : джа : һы : сы : тан : э : со : уас : со : йо]",
                "sound": ""
                },{
                "phrase": "Жасыңыз қаншада?",
                "translation": "몇 살 이세요?",
                "transcription": "[мёт : саль : и : сэ : йо]",
                "sound": ""
                },{
                "phrase": "Менің жасым …(сан)",
                "translation": "저는 … 살입니다",
                "transcription": "[чо : нын : … : саль : им : ни : да ]",
                "sound": ""
                }
              ]
    },
    
