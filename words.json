    "Емханада":{
      "title": "Емханада",
        
        
      "data":[
        
        {
          "phrase": "Дәрігер шақырыңыз (өтініш)",
          "translation": "언제 그랬습니까?",
          "transcription": "[ый : са : рыль : пуль : лё : чу : сэ: йо]",
          "sound": ""
        },
        {
          "phrase": "Қай жеріңіз ауырып тұр?",
          "translation": "어디가 아프십니까?",
          "transcription": "[о : ди : га : а : пы : щим : ник : ка]",
          "sound": "[]"
        },
        {
          "phrase": "Қандай көмек көрсетейін?",
          "translation": "뭘 도와 드릴까요?",
          "transcription": "[муоль : до : уа : ды : рильк : ка : йо]",
          "sound": ""
        },
        {
          "phrase": "Бұдан басқа еш жеріңіз ауырып тұрған жоқ па?",
          "translation": "그밖에 또 아픈 곳이 있습니까?",
          "transcription": "[кы : паг : ке : ддо : а : пын : го : щи : ис : сым : ник : ка ]",
          "sound": ""
        },
        {
          "phrase": "Қанша уақыт болды?",
          "translation": "언제 그랬습니까?",
          "transcription": "[он : джэ : кы : рэс : сым : ник : ка]",
          "sound": ""
        },
        {
          "phrase": "Қашаннан бері аурып жатыр?",
          "translation": "언제부터 아프셨습니까?",
          "transcription": "[он : джэ : пу : то : а : пы : щёс : сым : ник : ка]",
          "sound": ""
        },
        {
          "phrase": "Өзімді нашар сезініп тұрмын",
          "translation": "몸이 안 좋습니다",
          "transcription": "[мом : и : ан : чо : сым : ни : да]",
          "sound": ""
        },
        {
          "phrase": "Әлсіз болып тұрмын",
          "translation": "몸이 나른합니다",
          "transcription": "[мом : и : на : рын : хам : ни : да]",
          "sound": ""
        },
        {
          "phrase": "Басым айналып жатыр",
          "translation": "현기증이 납니다",
          "transcription": "[хён : ги : джың : и : нам : ни : да]",
          "sound": ""
        },
        {
          "phrase": "Тәбетім жоқ",
          "translation": "식욕이 없습니다",
          "transcription": "[щиг : ёг : и : оп : сым : ни : да]",
          "sound": ""
        },
        {
          "phrase": "Түнде ұйықтай алмаймын",
          "translation": "밤에 잠이 안 옵니다",
          "transcription": "[пам : э : чам : и : ан : ом : ни : да]",
          "sound": ""
        },
        {
          "phrase": "Дене температураңызды өлшеп көрейін",
          "translation": "체온을 재보겠습니다",
          "transcription": "[тчэ : он : ыль : чэ : по : гес : сым : ни : да]",
          "sound": ""
        },
        {
          "phrase": "Қан қысымыңызды өлшеп көрейін",
          "translation": "혈압을 재겠습니다",
          "transcription": "[хёль : ап : ыль : чэ : гес : сым : ни : да]",
          "sound": ""
        },
        {
          "phrase": "Емханаға жатуыңыз қажет",
          "translation": "입원해야만 합니다",
          "transcription": "[иб : уон : хэ : йа : ман : хам : ни : да]",
          "sound": ""
        }
      ]
    }
    
    
    
    
    
